local uis=game:GetService("UserInputService");


uis.InputBegan:connect(function(input)
	if input.KeyCode==Enum.KeyCode.A then
		local akeyboard = Instance.new("ScreenGui")
		local akey = Instance.new("TextBox")
		local UICorner = Instance.new("UICorner")


		akeyboard.Name = "akeyboard"
		akeyboard.Parent = game.CoreGui

		akey.Name = "akey"
		akey.Parent = akeyboard
		akey.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		akey.BorderSizePixel = 0
		akey.Position = UDim2.new(0.224276021, 0, 0.851088226, 0)
		akey.Size = UDim2.new(0, 30, 0, 27)
		akey.Font = Enum.Font.SourceSans
		akey.Text = "A"
		akey.TextColor3 = Color3.fromRGB(0, 0, 0)
		akey.TextSize = 39.000
		akey.Active = true
		akey.Draggable = false

		UICorner.Parent = akey 
	end
end)

 
uis.InputEnded:connect(function(input)
	if input.KeyCode==Enum.KeyCode.A then
		game:GetService("CoreGui").akeyboard:Destroy()
	end
end)

