local uis=game:GetService("UserInputService");


uis.InputBegan:connect(function(input)
	if input.KeyCode==Enum.KeyCode.One then

		local onekeyboard = Instance.new("ScreenGui")
		local onekey = Instance.new("TextBox")
		local UICorner = Instance.new("UICorner")


		onekeyboard.Name = "onekeyboard"
		onekeyboard.Parent = game.CoreGui
		

		onekey.Name = "onekey"
		onekey.Parent = onekeyboard
		onekey.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		onekey.BorderSizePixel = 0
		onekey.Position = UDim2.new(0.21133703, 0, 0.789232552, 0)
		onekey.Size = UDim2.new(0, 30, 0, 27)
		onekey.Font = Enum.Font.SourceSans
		onekey.Text = "1"
		onekey.TextColor3 = Color3.fromRGB(0, 0, 0)
		onekey.TextSize = 39.000
		onekey.Active = true
		onekey.Draggable = false

		UICorner.Parent = onekey
	end
end)


uis.InputEnded:connect(function(input)
	if input.KeyCode==Enum.KeyCode.One then
		game:GetService("CoreGui").onekeyboard:Destroy()
	end	
end)

