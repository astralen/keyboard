local uis=game:GetService("UserInputService");


uis.InputBegan:connect(function(input)
	if input.KeyCode==Enum.KeyCode.LeftShift then

		local keyboardshift = Instance.new("ScreenGui")
		local shiftkey = Instance.new("TextBox")
		local UICorner = Instance.new("UICorner")


		keyboardshift.Name = "keyboardshift"
		keyboardshift.Parent = game.CoreGui

		shiftkey.Name = "shiftkey"
		shiftkey.Parent = keyboardshift
		shiftkey.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		shiftkey.BorderSizePixel = 0
		shiftkey.Position = UDim2.new(0.192852736, 0, 0.882016063, 0)
		shiftkey.Size = UDim2.new(0, 67, 0, 27)
		shiftkey.Font = Enum.Font.SourceSans
		shiftkey.Text = "SHIFT"
		shiftkey.TextColor3 = Color3.fromRGB(0, 0, 0)
		shiftkey.TextSize = 30.000
		shiftkey.Active = true
		shiftkey.Draggable = false

		UICorner.Parent = shiftkey
	end
end)


uis.InputEnded:connect(function(input)
	if input.KeyCode==Enum.KeyCode.LeftShift then
		game:GetService("CoreGui").keyboardshift:Destroy()
	end
end)

